{
  "plugins": [
    "svelte3"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ]
  ,
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "overrides": [
    {
      "files": ["*.{svelte, ts}"],
      "processor": "svelte3/svelte3"
    }
  ]
}